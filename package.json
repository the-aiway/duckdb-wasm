{
    "name": "aiway-duckdb-wasm",
    "version": "1.11.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/duckdb/duckdb-wasm.git"
    },
    "private": true,
    "workspaces": [
        "packages/*",
        "examples/*"
    ],
    "main": "packages/duckdb-wasm/dist/duckdb-browser.cjs",
    "module": "packages/duckdb-wasm/dist/duckdb-browser.mjs",
    "types": "packages/duckdb-wasm/src/targets/duckdb.ts",
    "exports": {
        "./dist/duckdb-mvp.wasm": "./packages/duckdb-wasm/dist/duckdb-mvp.wasm",
        "./dist/duckdb-eh.wasm": "./packages/duckdb-wasm/dist/duckdb-eh.wasm",
        "./dist/duckdb-coi.wasm": "./packages/duckdb-wasm/dist/duckdb-coi.wasm",
        "./dist/*": "./packages/duckdb-wasm/dist/*",
        ".": {
            "browser": {
                "types": "./packages/duckdb-wasm/src/targets/duckdb.ts",
                "import": "./packages/duckdb-wasm/dist/duckdb-browser.mjs",
                "require": "./packages/duckdb-wasm/dist/duckdb-browser.cjs"
            },
            "node": {
                "types": "./packages/duckdb-wasm/src/targets/duckdb.ts",
                "import": "./packages/duckdb-wasm/dist/duckdb-node.cjs",
                "require": "./packages/duckdb-wasm/dist/duckdb-node.cjs"
            },
            "types": "./packages/duckdb-wasm/src/targets/duckdb.ts",
            "import": "./packages/duckdb-wasm/dist/duckdb-browser.mjs",
            "require": "./packages/duckdb-wasm/dist/duckdb-browser.cjs"
        }
    },
    "scripts": {
        "lint:fix": "yarn workspace @duckdb/duckdb-wasm lint --fix && yarn workspace @duckdb/benchmarks lint --fix && yarn workspace @duckdb/duckdb-wasm-shell lint --fix",
        "docs": "yarn workspace @duckdb/duckdb-wasm docs",
        "benchmarks:build:browser": "yarn workspace @duckdb/benchmarks build:browser",
        "benchmarks:build:node": "yarn workspace @duckdb/benchmarks build:node",
        "benchmarks:build": "yarn workspace @duckdb/benchmarks build"
    },
    "devDependencies": {
        "badge-maker": "^3.3.1"
    }
}
